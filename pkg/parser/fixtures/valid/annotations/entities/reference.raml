#%RAML 1.0 Library

uses:
  cti: ../cti.raml

types:
    EntityWithReference:
        (cti.cti): cti.a.p.entity_with_reference.v1.0
        properties:
            implicit_reference:
                type: cti.CTI
                (cti.reference): true
            single_reference:
                type: cti.CTI
                (cti.reference): cti.a.p.other_entity.v1.0
            multiple_references:
                type: cti.CTI
                (cti.reference):
                - cti.a.p.other_entity.v1.0
                - cti.a.p.sample_entity.v1.0

    EntityWithArrayReference:
        (cti.cti): cti.a.p.entity_with_array_reference.v1.0
        properties:
            array_reference:
                type: cti.CTI[]
                (cti.reference): cti.a.p.other_entity.v1.0
            array_references:
                type: cti.CTI[]
                (cti.reference):
                - cti.a.p.other_entity.v1.0
                - cti.a.p.sample_entity.v1.0
